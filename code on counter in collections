from collections import Counter

# Input
n = int(input())
shoes = list(map(int, input().split()))
shoe_count = Counter(shoes)

customers = int(input())
total_money = 0

for _ in range(customers):
    size, price = map(int, input().split())
    if shoe_count[size] > 0:
        total_money += price
        shoe_count[size] -= 1

print(total_money)
